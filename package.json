{
    "name": "@gik/tools-streamer",
    "version": "0.0.9",
    "description": "GIK's take on the Observable",
    "license": "MIT",
    "homepage": "https://github.com/gikmx/tools",
    "contributors": [
        "Héctor Menéndez <hector@gik.mx>"
    ],
    "keywords": [
        "rxjs",
        "functional programming",
        "observable",
        "streaming",
        "utility",
        "tools"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gikmx/tools-streamer"
    },
    "bugs": {
        "url": "https://github.com/gikmx/tools/issues"
    },
    "engines": {
        "node": ">=6.11.3",
        "npm": ">=5.4.2"
    },
    "main": "lib/streamer.js",
    "directories": {
        "test": "./__tests__",
        "src": "./src",
        "out": "./lib"
    },
    "scripts": {
        "docs": "gik-npm docs",
        "test": "gik-npm lint test && gik-npm test",
        "build": "gik-npm lint src && gik-npm build",
        "verify": "npm run build -s && npm run test -s",
        "start": "gik-npm start 'npm run verify -s'",
        "pub": "npm run verify -s && npm run docs -s && npm publish --access public --tag",
        "precommit": "npm run verify -s && gik-npm version patch && npm run docs -s",
        "pub:beta": "npm run verify -s && npm publish --access public --tag beta",
        "pub:latest": "npm run verify -s && npm publish --access public --tag latest"
    },
    "dependencies": {
        "@gik/tools-checker": "0.0.8",
        "@gik/tools-logger": "0.0.6",
        "@gik/tools-thrower": "^0.1.9",
        "rxjs": "^5.5.2"
    },
    "devDependencies": {
        "@gik/eslint-config": "0.0.4",
        "@gik/npm": "^0.1.4",
        "nodemon": "^1.12.1"
    }
}
